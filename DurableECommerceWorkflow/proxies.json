{
  "$schema": "http://json.schemastore.org/proxies",
  "proxies": {
    "proxy1": {
      "matchCondition": {
        "methods": [ "GET" ],
        "route": "/"
      },
      "debug": true,
      "backendUri": "http://localhost:54045/index.html"
    },
    "proxy2": {
      "matchCondition": {
        "methods": [ "GET" ],
        "route": "/images/{*restOfPath}"
      },
      "debug": true,
      "backendUri": "http://localhost:54045/images/{restOfPath}"
    }
  }
}